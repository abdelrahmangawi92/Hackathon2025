

<img src="music_note.png" id="musicNote" alt="Music Note" style="cursor:pointer;" />

<!-- Hidden Audio Player -->
<audio id="musicPlayer" preload="auto">
<source src="Not Like Us.mp3" type="audio/mp3">
Your browser does not support the audio element.
</audio>

<script type="text/javascript">
// Get the image element and the audio element
var musicNote = document.getElementById('musicNote');
var musicPlayer = document.getElementById('musicPlayer');

// Event listener to play the music when the image is clicked
musicNote.addEventListener('click', function() {
    musicPlayer.play();
});
</script>
